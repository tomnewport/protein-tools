file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Sargeant et al. - 2006 - Lineage-specific expansion of proteins exported to erythrocytes in malaria parasites.pdf:pdf},
issn = {1465-6914},
journal = {Genome biology},
keywords = {Amino Acid Sequence,Animals,Conserved Sequence,Erythrocytes,Erythrocytes: parasitology,Humans,Malaria, Falciparum,Malaria, Falciparum: blood,Multigene Family,Open Reading Frames,Plasmodium falciparum,Plasmodium falciparum: genetics,Protozoan Proteins,Protozoan Proteins: blood,Protozoan Proteins: genetics,Software},
