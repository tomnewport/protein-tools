journal = {Cell host \& microbe},
keywords = {Animals,Aspartic Acid Endopeptidases,Aspartic Acid Endopeptidases: metabolism,Carrier Proteins,Carrier Proteins: metabolism,Falciparum,Falciparum: metabolism,Humans,Malaria,Phosphatidylinositol Phosphates,Phosphatidylinositol Phosphates: metabolism,Plasmodium falciparum,Plasmodium falciparum: metabolism,Protein Transport,Protein Unfolding,Protozoan Proteins,Protozoan Proteins: chemistry,Protozoan Proteins: metabolism},
month = nov,
number = {5},
pages = {717--29},
pmid = {23159060},
title = {{Uncovering common principles in protein export of malaria parasites.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23159060},
volume = {12},
year = {2012}
}
@article{Treeck2011,
abstract = {Plasmodium falciparum and Toxoplasma gondii are obligate intracellular apicomplexan parasites that rapidly invade and extensively modify host cells. Protein phosphorylation is one mechanism by which these parasites can control such processes. Here we present a phosphoproteome analysis of peptides enriched from schizont stage P. falciparum and T. gondii tachyzoites that are either "intracellular" or purified away from host material. Using liquid chromatography-tandem mass spectrometry, we identified over 5,000 and 10,000 previously unknown phosphorylation sites in P. falciparum and T. gondii, respectively, revealing that protein phosphorylation is an extensively used regulation mechanism both within and beyond parasite boundaries. Unexpectedly, both parasites have phosphorylated tyrosines, and P. falciparum has unusual phosphorylation motifs that are apparently shaped by its A:T-rich genome. This data set provides important information on the role of phosphorylation in the host-pathogen interaction and clues to the evolutionary forces operating on protein phosphorylation motifs in both parasites.},
author = {Treeck, Moritz and Sanders, John L and Elias, Joshua E and Boothroyd, John C},
doi = {10.1016/j.chom.2011.09.004},
file = {:Users/tom/Library/Application Support/Mendeley Desktop/Downloaded/Treeck et al. - 2011 - The phosphoproteomes of Plasmodium falciparum and Toxoplasma gondii reveal unusual adaptations within and beyond.pdf:pdf},
