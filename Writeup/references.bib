month = oct,
number = {4},
pages = {410--9},
pmid = {22018241},
publisher = {Elsevier},
title = {{The phosphoproteomes of Plasmodium falciparum and Toxoplasma gondii reveal unusual adaptations within and beyond the parasites' boundaries.}},
