issn = {1530-6860},
journal = {FASEB journal : official publication of the Federation of American Societies for Experimental Biology},
keywords = {cytoadherence,during the intraerythrocytic cycle,exported proteins,falciparum completely refurbishes the,human erythro-,interactions,malaria,plasmodium,protein structure},
month = jun,
