journal = {Cellular microbiology},
keywords = {Cells, Cultured,Erythrocyte Membrane,Erythrocyte Membrane: parasitology,Erythrocyte Membrane: physiology,Erythrocytes,Erythrocytes: parasitology,Erythrocytes: pathology,Green Fluorescent Proteins,Host-Parasite Interactions,Host-Parasite Interactions: physiology,Humans,Plasmodium falciparum,Plasmodium falciparum: pathogenicity,Plasmodium falciparum: physiology,Protein Transport,Protein Transport: physiology,Protozoan Proteins,Protozoan Proteins: physiology},
month = aug,
