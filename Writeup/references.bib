issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Animals,Databases,Genetic,Genome,Genomics,Plasmodium,Plasmodium: genetics,Plasmodium: growth \& development,Plasmodium: metabolism,Protozoan,Protozoan Proteins,Protozoan Proteins: genetics,Protozoan Proteins: physiology,Transcription},
